{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "routes": [
    {
      "src": "/resume.pdf",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "application/pdf"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
